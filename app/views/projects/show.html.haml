- provide :body_class, "nav-md"

.right_col{:role => "main", :style => "min-height: 1264px;"}
  %div
    .page-title
      .title_left
        %h3 #{@project.name}
    .clearfix

    .row
      .col-md-4.col-sm-4.col-xs-12
        .x_panel.tile.fixed_height_320
          .x_title
            %h2 Summary
            %ul.nav.navbar-right.panel_toolbox
              %li
                %a.collapse-link
                  %i.fa.fa-chevron-up
              %li.dropdown
                %a.dropdown-toggle{"aria-expanded" => "false", "data-toggle" => "dropdown", :href => "#", :role => "button"}
                  %i.fa.fa-wrench
                %ul.dropdown-menu{:role => "menu"}
                  %li
                    %a{:href => "#"} Settings 1
                  %li
                    %a{:href => "#"} Settings 2
              %li
                %a.close-link
                  %i.fa.fa-close
            .clearfix
          .x_content
            %h4 Project participation
            .widget_summary
              .w_left.w_25
                %span 0.1.5.2
              .w_center.w_55
                .progress
                  .progress-bar.bg-green{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", :style => "width: 66%;"}
                    %span.sr-only 60% Complete
              .w_right.w_20
                %span 123k
              .clearfix
            .widget_summary
              .w_left.w_25
                %span 0.1.5.3
              .w_center.w_55
                .progress
                  .progress-bar.bg-green{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", :style => "width: 45%;"}
                    %span.sr-only 60% Complete
              .w_right.w_20
                %span 53k
              .clearfix
            .widget_summary
              .w_left.w_25
                %span 0.1.5.4
              .w_center.w_55
                .progress
                  .progress-bar.bg-green{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", :style => "width: 25%;"}
                    %span.sr-only 60% Complete
              .w_right.w_20
                %span 23k
              .clearfix
            .widget_summary
              .w_left.w_25
                %span 0.1.5.5
              .w_center.w_55
                .progress
                  .progress-bar.bg-green{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", :style => "width: 5%;"}
